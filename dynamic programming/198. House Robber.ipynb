    "class Solution:\n",
    "    def rob(self, nums):\n",
    "        \"\"\"\n",
    "        :type nums: List[int]\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        \n",
    "        last, now = 0, 0\n",
    "        \n",
    "        for i in nums: \n",
    "            \n",
    "            last, now = now, max(last + i, now)\n",
    "                \n",
    "        return now"
   ]
  }
